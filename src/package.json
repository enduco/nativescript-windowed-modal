{
  "name": "nativescript-windowed-modal",
  "version": "6.2.8",
  "description": "Consistent modals for Android and iOS.",
  "main": "index",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "5.0.0",
      "ios": "5.0.0"
    }
  },
  "scripts": {
    "build": "npm i && ts-patch install && tsc",
    "demo.ios": "cd ../demo && tns run ios",
    "demo.android": "cd ../demo && tns run android",
    "demo.addplugin": "cd ../demo && npm run add.plugin",
    "demo.ng.ios": "cd ../demo-ng && tns run ios",
    "demo.ng.android": "cd ../demo-ng && tns run android",
    "demo.ng.addplugin": "cd ../demo-ng && npm run add.plugin",
    "ci.build": "tsc",
    "ci.tslint": "cd .. && tslint '**/*.ts'"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Typescript",
    "Android",
    "iOS",
    "Modal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mukaschultze/nativescript-windowed-modal"
  },
  "author": {
    "name": "Samuel Schultze",
    "email": "samuelschultze@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/mukaschultze/nativescript-windowed-modal/issues"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/mukaschultze/nativescript-windowed-modal",
  "readmeFilename": "README.md",
  "devDependencies": {
    "prompt": "^1.0.0",
    "rimraf": "^2.6.3",
    "semver": "^5.5.0",
    "@nativescript/core": "~7.0.2",
    "@nativescript/types": "~7.0.0",
    "@nativescript/webpack": "~3.0.0",
    "tslint": "^5.12.1",
    "ts-node": "^9.0.0",
    "ts-patch": "^1.3.0",
    "typescript": "~3.9.0"
  },
  "bootstrapper": "nativescript-plugin-seed"
}
